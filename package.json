{
  "name": "afs-front",
  "version": "1.0.1",
  "private": false,
  "description": "反欺诈系统",
  "homepage": ".",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "build:dev": "cross-env UMI_ENV=dev max build",
    "build:devK8s": "cross-env UMI_ENV=dev max build && npm run zip",
    "build:k8s": "max build && npm run zip",
    "build:post": "git pull && max build && tar -czvf data-afsfront-front.tar.gz --transform 's/^dist/data-afsfront-front/' dist",
    "build:prod": "cross-env UMI_ENV=prod max build",
    "build:sit": "cross-env UMI_ENV=sit max build",
    "dev": "npm run start:dev",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "new:page": "node ./scripts/newPage",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=local max dev",
    "start:bbit": "cross-env PORT=8085 UMI_ENV=bbit max dev",
    "start:dev": "cross-env PORT=8085 REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:sit": "cross-env PORT=8085 UMI_ENV=sit max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit",
    "zip": "powershell Compress-Archive -Force -Path dist/* -DestinationPath dist/dist.zip && powershell Get-ChildItem -Path dist -Exclude dist.zip | powershell Remove-Item -Path dist -Exclude dist.zip -Recurse -Force"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@alita/plugins": "^3.3.1",
    "@ant-design/icons": "^4.8.0",
    "@ant-design/pro-components": "^2.6.2",
    "@ant-design/use-emotion-css": "1.0.4",
    "@formily/antd-v5": "^1.1.0-beta.3",
    "@formily/core": "^2.2.24",
    "@formily/react": "^2.2.24",
    "@formily/reactive": "^2.2.24",
    "@textea/json-viewer": "^4.0.1",
    "@umijs/route-utils": "^2.2.2",
    "antd": "^5.5.2",
    "canvg": "^4.0.3",
    "classnames": "^2.3.2",
    "crypto-js": "^4.2.0",
    "dagre": "^0.8.5",
    "dayjs": "^1.11.7",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "eve": "^0.5.4",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "mathjs": "^12.1.0",
    "omit.js": "^2.0.2",
    "path": "^0.12.7",
    "qs": "^6.11.2",
    "rc-menu": "^9.8.2",
    "rc-queue-anim": "^2.0.0",
    "rc-util": "^5.27.2",
    "react": "^18.2.0",
    "react-dev-inspector": "^1.8.4",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-helmet-async": "^1.3.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@testing-library/react": "^13.4.0",
    "@types/classnames": "^2.3.1",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/file-saver": "^2.0.7",
    "@types/history": "^4.7.11",
    "@types/html2canvas": "^1.0.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.0.52",
    "@umijs/max": "^4.0.52",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "husky": "^7.0.4",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.4",
    "swagger-ui-dist": "^4.15.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "umi-presets-pro": "^2.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://100.74.128.139:8060/repository/npm-hosted"
  }
}
